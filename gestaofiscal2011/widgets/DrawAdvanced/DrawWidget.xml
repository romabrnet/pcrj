<?xml version="1.0" ?>
<configuration>
    <!-- Map and geometry service settings -->
    <!-- <geometryservice>http://sampleserver3.arcgisonline.com/ArcGIS/rest/services/Geometry/GeometryServer</geometryservice> -->
    <geometryservice>http://arcgis.ecan.govt.nz/ArcGIS/rest/services/Geometry/GeometryServer</geometryservice>
    <spatialref>2193</spatialref>
    
    <!-- If utilising an arcgis server version 10 or higher for queries, etc, the following setting should be set to true.  If utilising an ArcGIS 9.3x server, set to false -->
	<useAMF>false</useAMF>
    
    <!-- Graphics layer name -->
    <graphicsLayerName alias="Drawn Graphics" ></graphicsLayerName>
    
    	<!-- Feature creation functionality -->
    <createoptions>
        <polygondrawtools>polygon,freehandpolygon,extent,autoComplete,circle,ellipse</polygondrawtools>
        <polylinedrawtools>polyline,freehandpolyline,line</polylinedrawtools>
    </createoptions>
    
    <!-- Toolbar functionality -->
    <toolbarvisible>true</toolbarvisible>
    <toolbarcutvisible>true</toolbarcutvisible>
    <toolbarmergevisible>true</toolbarmergevisible>
    <toolbarreshapevisible>true</toolbarreshapevisible>   
    <showMeasurements>true</showMeasurements>
    
    <!-- Distance units used for meaurement unit conversion -->
    <distanceunits>
        <distanceunit abbr="m" conversion="1" precision="2">Metros</distanceunit>
        <distanceunit abbr="km" conversion=".001" precision="2">Quilômetros</distanceunit>
        <distanceunit abbr="ft" conversion="3.2808" precision="2">Pés</distanceunit>
        <distanceunit abbr="mi" conversion="0.000621" precision="2">Milhas</distanceunit>
    </distanceunits>
    <defaultdistanceunit>0</defaultdistanceunit>

    <!-- Area units used for meaurement unit conversion -->
    <areaunits>
        <areaunit abbr="m²" conversion="1" precision="0">Metros Quadrado</areaunit>
        <areaunit abbr="km²" conversion="0.000001" precision="3">Quilômetros Quadrado</areaunit>
        <areaunit abbr="ft²" conversion="10.763910417" precision="0">Pés Quadrado</areaunit>
        <areaunit abbr="mi²" conversion="0.0000003861021" precision="3">Milhas Quadrada</areaunit>
        <areaunit abbr="ac" conversion="0.00024710538147" precision="0">Acres</areaunit>
        <areaunit abbr="ha" conversion="0.0001" precision="0">Hectares</areaunit>
    </areaunits>
    <defaultareaunit>0</defaultareaunit>

    <!-- Buffer units used for buffer generation -->
	<bufferranges>
		<bufferrange distance="50">50 metros</bufferrange>
		<bufferrange distance="100">100 metros</bufferrange>
		<bufferrange distance="500">500 metros</bufferrange>
		<bufferrange distance="1000">1000 metros</bufferrange>
	</bufferranges>	
	<defaultbufferindex>3</defaultbufferindex>

    <!-- Widget labels -->
	<labels>
	    <!-- Widget menu labels -->
		<drawlabel>Ferramentas de Desenho</drawlabel>
		<measurementoptionslabel>Configurações de Medição e Buffer</measurementoptionslabel>
		<selectedgraphicslabel>Gráficos Selecionados</selectedgraphicslabel>
		<graphicslistlabel>Lista de Gráficos do Mapa</graphicslistlabel>
		<openlabel>Abrir Camada de Gráficos salva</openlabel>
		<savelabel>Salvar Camada de Gráficos</savelabel>
		
		<!-- Measurement option labels --> 
		<measurementslabel>Configurações de Medição</measurementslabel>
		<showmeasurementslabel>Mostrar Medidas com Gráficos:</showmeasurementslabel>
		<distanceunitslabel>Unidades de Distância:</distanceunitslabel>
		<distanceunitstooltip>Escolha as unidades medidas para exibir distâncias em</distanceunitstooltip>
		<areaunitslabel>Unidades de Área:</areaunitslabel>
		<areaunitstooltip>Escolha as unidades medidas para exibir áreas em</areaunitstooltip>
		<arealabel>Área: </arealabel>
		<perimeterlabel>Perímetro: </perimeterlabel>	
		<lengthlabel>Comprimento: </lengthlabel>
	
		<!-- Buffer option labels --> 
		<bufferslabel>Configurações de Buffer</bufferslabel>
		<showbufferslabel>Gerar Buffer para as camadas desenhadas</showbufferslabel>
		<bufferunitlabel>Buffer:</bufferunitlabel>
		<bufferunittooltip>Selecione a distância para o buffer de gráficos por</bufferunittooltip>
		<bufferdistancelabel>Distância (m):</bufferdistancelabel>
		<bufferdistancetooltip>Especifique a distância exata que você deseja usar ao gerar anéis de buffer</bufferdistancetooltip>
		<buffernumberlabel>Número de Anéis:</buffernumberlabel>
		<buffernumbertooltip>Escolha o número de anéis de buffer para gerar</buffernumbertooltip>
		<generatebufferlabel>Gerar Buffers com Gráficos</generatebufferlabel>
		<unionbufferslabel>Mesclar buffers:</unionbufferslabel>
		<unionbufferstooltip>Marque para mesclar os limites dos buffers gerados.</unionbufferstooltip>
		
		<!-- Toolbar labels --> 
		<pointToolLabel>Desenhar Ponto</pointToolLabel>
		<lineToolLabel>Desenhar Linha</lineToolLabel>
		<freehandLineToolLabel>Desenhar Linha à Mão Livre</freehandLineToolLabel>
		<polygonToolLabel>Desenhar Polígono</polygonToolLabel>
		<freehandPolygonToolLabel>Desenhar Polígono à Mão Livre</freehandPolygonToolLabel>
		<rectangleToolLabel>Desenhar Retângulo</rectangleToolLabel>
		<circleToolLabel>Desenhar Círculo</circleToolLabel>
		<ellipseToolLabel>Desenhar Elipse</ellipseToolLabel>
		<textToolLabel>Adicionar Texto</textToolLabel>
		<leaderTextToolLabel>Adicionar texto com guia</leaderTextToolLabel>
		<selectToolLabel>Selecionar Gráficos</selectToolLabel>
		<toolOptionsLabel>Opões para Definir padrão de simbologia</toolOptionsLabel>
		<clearToolLabel>Remover todos os Gráficos</clearToolLabel>
		<clearSelectionToolLabel>Desmarcar Gráficos</clearSelectionToolLabel>
	
		<!-- Interactive Measurement Label Settings -->
		<segmentlengthlabel>Comprimento do segmento da linha:</segmentlengthlabel>
		<totallengthlabel>Comprimento total da linha:</totallengthlabel>
	
		<!-- Context menu labels -->
		<clearfeaturelabel>Remover feição</clearfeaturelabel>
	</labels>
	
	
	<!-- Symbol Templates -->
	<defaulttemplates>
		<!-- Simple Marker Templates -->
		<template name="Círculo" description="Marcador círculo básico" groupname="Marcadores Básicos" drawingtool="mappoint" type="marker" style="circle" alpha="1" colour="0xFF0000" size="15" angle="0" xoffset="0" yoffset="0" outlinewidth="2" outlinecolour="0xFFFFFF" outlinestyle="solid" outlinealpha="1" /> 
		<template name="Triângulo" description="Marcador triángulo básico" groupname="Marcadores Básicos" drawingtool="mappoint" type="marker" style="triangle" alpha="1" colour="0x0000FF" size="15" angle="0" xoffset="0" yoffset="0" outlinewidth="2" outlinecolour="0xFFFFFF" outlinestyle="solid" outlinealpha="1" /> 
		<template name="Quadrado" description="Marcador quadrado básico" groupname="Marcadores Básicos" drawingtool="mappoint" type="marker" style="square" alpha="1" colour="0x33CCEE" size="15" angle="0" xoffset="0" yoffset="0" outlinewidth="2" outlinecolour="0x000000" outlinestyle="solid" outlinealpha="1" /> 
	
		<!-- Simple Line Templates -->
		<template name="Linha Vermelha" description="Linha vermelha básica" groupname="Linhas Básicas" drawingtool="pointToPointLine" type="line" style="solid" alpha="1" colour="0xFF0000" width="1" /> 
		<template name="Linha Azul" description="Linha azul básica" groupname="Linhas Básicas" drawingtool="pointToPointLine" type="line" style="solid" alpha="1" colour="0x0000FF" width="1" /> 
		<template name="Linha Verde" description="Linha verde básica" groupname="Linhas Básicas" drawingtool="pointToPointLine" type="line" style="solid" alpha="1" colour="0x008000" width="1" /> 
		<template name="Linha Branca" description="Linha branca básica" groupname="Linhas Básicas" drawingtool="pointToPointLine" type="line" style="solid" alpha="1" colour="0xFFFFFF" width="2" /> 
		<template name="Linha tracejada" description="Linha preta tracejada básica" groupname="Linhas Básicas" drawingtool="pointToPointLine" type="line" style="dash" alpha="1" colour="0x000033" width="2" /> 
		<template name="Linha pontilhada" description="Linha branca pontilhada básica" groupname="Linhas Básicas" drawingtool="pointToPointLine" type="line" style="dot" alpha="1" colour="0x000033" width="2" /> 

		<!-- Simple Fill Templates -->
		<template name="Contorno" description="Somente contorno" groupname="Áreas Básicas" drawingtool="pointToPointPolygon" type="fill" style="null" alpha="0" colour="0x000000" outlinewidth="1" outlinecolour="0x000000" outlinestyle="solid" outlinealpha="1" /> 
		<template name="Vermelho" description="Preenchimento vermelho fosco" groupname="Áreas Básicas" drawingtool="pointToPointPolygon" type="fill" style="solid" alpha="0.5" colour="0xE33638" outlinewidth="1" outlinecolour="0x000000" outlinestyle="solid" outlinealpha="1" /> 
		<template name="Lago" description="Preencimento azul fosco com contorno azul escuro" groupname="Áreas Básicas" drawingtool="pointToPointPolygon" type="fill" style="solid" alpha="0.5" colour="0x7EC0EE" outlinewidth="1" outlinecolour="0x4876FF" outlinestyle="solid" outlinealpha="1" /> 
		<template name="Cinza" description="Preenchimento cinza" groupname="Áreas Básicas" drawingtool="pointToPointPolygon" type="fill" style="solid" alpha="0.5" colour="0xC2C2C2" outlinewidth="1" outlinecolour="0x000000" outlinestyle="solid" outlinealpha="1" /> 
		<template name="Hachura" description="Preencimento preto diagonal tracejado" groupname="Áreas Básicas" drawingtool="pointToPointPolygon" type="fill" style="diagonalcross" alpha="1" colour="0x000033" outlinewidth="1" outlinecolour="0x000033" outlinestyle="solid" outlinealpha="1" /> 
		<template name="Verde" description="Preenchimento verde, parcialmente transparente e sem contorno" groupname="Áreas Básicas" drawingtool="pointToPointPolygon" type="fill" style="solid" alpha="0.5" colour="0x008B00" outlinewidth="0" outlinecolour="0xFFFFFF" outlinestyle="none" outlinealpha="0" /> 

		<!-- Text Templates -->
		<template name="Preto Simples" description="Rótulo com fonte preta básica" groupname="Texto Básico" drawingtool="text" type="text" font="Arial" fontsize="14" alpha="1" bold="true" italic="false" underline="false" colour="0x000000" border="false" borderColour="0x000000" background="false" 
			backgroundColour="0x000000" placement="middle" angle="0" xoffset="0" yoffset="0" leftmargin="0" rightmargin="0" align="center" />
		<template name="Preto 28" description="Rótulo com fonte preta básica" groupname="Texto Básico" drawingtool="text" type="text" font="Arial" fontsize="28" alpha="1" bold="true" italic="false" underline="false" colour="0x000000" border="false" borderColour="0x000000" background="false" 
			backgroundColour="0x000000" placement="middle" angle="0" xoffset="0" yoffset="0" leftmargin="0" rightmargin="0" align="center" />
		<template name="Vermelho 14" description="Rótulo com fonte vermelha" groupname="Texto Básico" drawingtool="text" type="text" font="Arial" fontsize="14" alpha="1" bold="true" italic="false" underline="false" colour="0xf30733" border="false" borderColour="0xf30733" background="false" 
			backgroundColour="0xf30733" placement="middle" angle="0" xoffset="0" yoffset="0" leftmargin="0" rightmargin="0" align="center" />
	</defaulttemplates>
	
	<!-- Help Location Settings -->
	<helplocationurl></helplocationurl>

	<projections>
		<projection wkid="29193" name="SAD_1969_UTM_Zone_23S" />
	</projections>
	
	<!-- IMPORT / Url of tools that import Zipped Shapefiles and CAD files -->
	<importurltool>http://geosmf.rio.rj.gov.br:6080/arcgis/rest/services/ImportCadService/GPServer</importurltool>
</configuration>