<?xml version="1.0" ?>
<configuration label="Localizar Endereço"> <!-- Configuração da Widget de Localizar Endereço -->
    <!-- Endereço do serviço de localização -->
    <locator>http://pgeo2.rio.rj.gov.br/ArcGIS/rest/services/Geocode/DBO.Loc_composto/GeocodeServer</locator>
    <!-- Pontuação Mínima da localização -->
    <minscore>40</minscore>
    <!-- Escala de zoom -->
    <zoomscale>10000</zoomscale>
    <!-- Labels da Aplicação -->
    <labels>
    	<!-- Label do Primeiro Label -->
    	<addresslabel> Endereço </addresslabel>
    	<!-- Label do Segundo Label -->
    	<addresstitle> Entre com o endereço</addresstitle>
    	<!-- Label da Aba de Resultados -->
    	<resultslabel> Resultados </resultslabel>
    	<!-- Label das Coordenadas -->
    	<coordinateslabel> Coordenadas </coordinateslabel>
    	<!-- Texto de Localização sem resultado-->
    	<noresultlabel> A localização correspondente não foi encontrada </noresultlabel>
    	<!-- Label do Primeiro Botão da Barra de Ferramentas -->
    	<submitlabel> Localizar </submitlabel>
    	<!-- Label do Segundo Botão da Barra de Ferramentas -->
    	<clearlabel> Limpar </clearlabel>
    	<!-- Texto enquanto carrega a aplicação -->
    	<loadinglabel> Carregando... </loadinglabel>
    	<!-- Texto explicativo de Coordenadas -->
    	<coordinatestitle> Informe as coordenadas SAD69 / UTM zone 23S</coordinatestitle>
    	<!-- Texto da coordenada x -->
    	<xlabel> X: </xlabel>
    	<!-- Texto da coordenada y -->
    	<ylabel> Y: </ylabel>
    	<!-- Valor padrão da coordenada X -->
    	<xexamplevalue> 681551.652 </xexamplevalue>
    	<!-- Valor padrão da coordenada Y -->
    	<yexamplevalue> 7465151.776 </yexamplevalue>
    	<!-- Label do Locais Encontrados -->
    	<locationslabel> Locais encontrados </locationslabel>
    	
    </labels>
</configuration>

<!--
    See Locate widget tag reference at
    http://links.esri.com/locatewidget
-->
